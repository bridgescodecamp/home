{
  "name": "BridgesHome",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "css": "npm-run-all --parallel css-compile* --sequential css-minify*",
    "css-compile": "node-sass --output-style expanded --precision 6 assets/sass/main-build.scss assets/css/main-1.css",
    "css-minify": "cleancss --level 1 --output assets/css/main-1.min.css assets/css/main-1.css"
  },
  "repository": "http://git.cua.edu/BridgesHome.git",
  "author": "Tyler Carver <carver230620@cua.edu>",
  "license": "MIT",
  "dependencies": {
    "clean-css-cli": "^4.2.1",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3"
  }
}
